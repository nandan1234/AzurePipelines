{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "learnfactory1234"
		},
		"learnazure1234_pooldb_service_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'learnazure1234_pooldb_service'"
		},
		"learndb1234_service_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'learndb1234_service'"
		},
		"learndb1_service_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'learndb1_service'"
		},
		"learndb1_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://learndb1.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "learnfactory1234_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"learnazure1234_pooldb_service_connectionString": {
						"value": "[parameters('learnazure1234_pooldb_service_connectionString')]"
					},
					"learndb1234_service_connectionString": {
						"value": "[parameters('learndb1234_service_connectionString')]"
					},
					"learndb1_service_accountKey": {
						"value": "[parameters('learndb1_service_accountKey')]"
					},
					"learndb1_service_properties_typeProperties_url": {
						"value": "[parameters('learndb1_service_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "learnfactory1234_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'learnfactory1234_ArmTemplate_0')]"
			]
		}
	]
}